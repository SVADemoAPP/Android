            <intent-filter>
                <action
                    android:name="com.hck.imagemap.myService"
                    android:process=":remote" />
            </intent-filter>
        </service>

        <activity
            android:name="com.hck.imagemap.WifiStrActivity"
            android:label="@string/title_activity_wifi_str"
            android:theme="@android:style/Theme.NoTitleBar" >
