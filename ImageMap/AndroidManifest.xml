        <activity
            android:name="com.hck.imagemap.ConfigActivity"
            android:label="@string/title_activity_config"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity android:name="com.hck.imagemap.LocationDelayActivity" >
        </activity>
        <activity android:name="com.hck.imagemap.PushMessageActivity" >
